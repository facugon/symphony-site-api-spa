{% extends 'IARMainBundle:Admin:layout.html.twig' %}

{% block main -%}
    <h1>Brand</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Disabled</th>
                <td>{{ entity.disabled }}</td>
            </tr>
            <tr>
                <th>Logofilename</th>
                <td>{{ entity.logoFilename }}</td>
            </tr>
            <tr>
                <th>Logox</th>
                <td>{{ entity.logoX }}</td>
            </tr>
            <tr>
                <th>Logoy</th>
                <td>{{ entity.logoY }}</td>
            </tr>
            <tr>
                <th>Logosizex</th>
                <td>{{ entity.logoSizeX }}</td>
            </tr>
            <tr>
                <th>Logosizey</th>
                <td>{{ entity.logoSizeY }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_brand') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_brand_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}

{% extends 'IARMainBundle:Admin:layout.html.twig' %}

{% block main -%}
    <h1>Proveedor list</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Razonsocial</th>
                <th>Direccion</th>
                <th>Telefono</th>
                <th>Email</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('admin_proveedor_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.razonSocial }}</td>
                <td>{{ entity.direccion }}</td>
                <td>{{ entity.telefono }}</td>
                <td>{{ entity.email }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('admin_proveedor_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('admin_proveedor_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('admin_proveedor_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
